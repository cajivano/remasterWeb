<h3><i class="fas fa-users"></i> MODIFICANDO CLIENTE: </h3>
     <div class="card">
      <div class="card-body">

        <form action="/update/<%= data.id %>" method="post">
          <div class="form-group">
            <input type="text" name="nombre" value="<%= data.nombre %>" class="form-control" autofocus>
          </div>
          <div class="form-group">
            <input type="text" name="apellido" value="<%= data.apellido %>" class="form-control">
          </div>
          <div class="form-group">
            <input type="text" name="celular" value="<%= data.celular %>" class="form-control">
          </div>

          <button type="submit" class="btn btn-primary">
            Modificar cliente
          </button>
        </form>
        </div>
      </div>
    </div>
